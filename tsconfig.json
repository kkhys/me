{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs", // eslint を実行するとエラーになるので追加
    "**/*.mjs", // 同上
    ".next/types/**/*.ts",
    "/Users/kh/IdeaProject_p/me/.next/types/**/*.ts" // TODO: なぜか追加されるので調査する
  ],
  "exclude": ["node_modules"],
  "extends": "./tsconfig.paths.json"
}
