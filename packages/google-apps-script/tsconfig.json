{
  "extends": "@kkhys/tsconfig/vite.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "baseUrl": ".",
    "paths": {
      "#/*": ["src/*"]
    },
    "types": ["google-apps-script"]
  },
  "include": ["src", "vite.config.mts", "vite-env.d.ts"],
  "exclude": ["node_modules"]
}
