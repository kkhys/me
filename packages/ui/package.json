{
  "name": "@kkhys/ui",
  "private": true,
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/styles.css": "./dist/styles.css"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"vite build --watch\"",
    "build": "run-p build:*",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist node_modules. .turbo",
    "storybook:dev": "storybook dev -p 6007 --ci --quiet",
    "storybook:build": "storybook build --quiet",
    "storybook:move": "mv storybook-static ../../storybook-static/ui"
  },
  "dependencies": {
    "@heroicons/react": "2.0.17",
    "clsx": "1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@kkhys/tailwind-config": "workspace:*",
    "@kkhys/tsconfig": "workspace:*",
    "@storybook/addon-essentials": "7.0.5",
    "@storybook/addon-interactions": "7.0.5",
    "@storybook/addon-links": "7.0.5",
    "@storybook/blocks": "7.0.5",
    "@storybook/react": "7.0.5",
    "@storybook/react-vite": "7.0.5",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/node": "18.15.11",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "3.0.0",
    "autoprefixer": "10.4.14",
    "concurrently": "8.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.22",
    "storybook": "7.0.5",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4",
    "vite": "4.2.0"
  }
}
