    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "3.10.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "semantic-release": "20.1.1",
