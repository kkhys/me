{
  "name": "me",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "run-p -c -l lint:**",
    "lint:prettier": "prettier --ignore-path .gitignore --check . \"!pnpm-lock.yaml\"",
    "lint:eslint": "next lint --ignore-path .gitignore",
    "fix": "run-s -l fix:**",
    "fix:prettier": "prettier --ignore-path .gitignore --write . \"!pnpm-lock.yaml\"",
    "fix:eslint": "next lint --ignore-path .gitignore --fix",
    "postinstall": "husky install"
  },
  "dependencies": {
    "clsx": "1.2.1",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@next/eslint-plugin-next": "13.2.3",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "autoprefixer": "10.4.14",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.35.0",
