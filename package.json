{
  "name": "me",
  "private": true,
  "scripts": {
    "dev": "turbo dev --parallel",
    "build": "turbo build",
    "clean": "git clean -xdf node_modules storybook-static",
    "clean:workspace": "turbo clean",
    "storybook:dev": "turbo run storybook:dev",
    "storybook:build": "turbo run storybook:build",
    "storybook:move": "turbo run storybook:move --filter=storybook && turbo run storybook:move --filter=!storybook",
    "format": "prettier --ignore-path .gitignore --write . \"!pnpm-lock.yaml\"",
    "lint": "turbo lint && manypkg check",
    "lint:fix": "turbo lint:fix && manypkg fix",
    "type-check": "turbo type-check",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@changesets/cli": "2.26.1",
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@ianvs/prettier-plugin-sort-imports": "4.0.2",
    "@kkhys/eslint-config": "0.1.0",
    "@kkhys/tsconfig": "0.1.0",
    "@manypkg/cli": "0.20.0",
    "@types/prettier": "2.7.3",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.41.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "turbo": "1.10.1",
    "typescript": "5.1.3"
  },
  "packageManager": "pnpm@8.4.0",
  "engines": {
    "node": "18",
    "pnpm": ">=8.4.0",
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!"
  },
  "volta": {
    "node": "18.16.0"
  },
  "devDependencies": {
    "playwright": "1.34.3"
  }
}
