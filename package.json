{
  "name": "me",
  "private": true,
  "scripts": {
    "commit": "cz",
    "format": "prettier --ignore-path .gitignore --write . \"!pnpm-lock.yaml\""
  },
  "packageManager": "pnpm@8.4.0",
  "engines": {
    "node": "18",
    "pnpm": ">=8.4.0",
    "npm": "use pnpm instead of npm please!",
    "yarn": "use pnpm instead of yarn please!"
  },
  "volta": {
    "node": "18.17.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "prettier": "3.0.1",
    "turbo": "1.10.12",
    "typescript": "5.1.6"
  }
}
