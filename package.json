{
  "name": "me",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules storybook-static",
    "storybook:dev": "turbo run storybook:dev",
    "storybook:build": "turbo run storybook:build",
    "storybook:move": "turbo run storybook:move --filter=storybook && turbo run storybook:move --filter=!storybook",
    "format": "prettier --ignore-path .gitignore --write . \"!pnpm-lock.yaml\" \"!pnpm-workspace.yaml\"",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "turbo": "1.9.1"
  },
  "packageManager": "pnpm@8.3.1",
  "engines": {
    "node": "18",
    "pnpm": "8.3.1",
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!"
  },
  "volta": {
    "node": "18.16.0"
  }
}
